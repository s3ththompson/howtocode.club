{
  "name": "how-to-code",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bankai build index.js && cp netlify.toml dist",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "dependencies": {
    "choo": "^6.12.1",
    "choo-service-worker": "^2.4.0",
    "choo-store": "^1.0.0",
    "gr8": "^3.1.5",
    "ress": "^1.2.2",
    "sheetify": "^7.3.3",
    "tachyons": "^4.10.0"
  },
  "devDependencies": {
    "bankai": "^9.14.0",
    "choo-devtools": "^2.5.1",
    "choo-scaffold": "^1.2.0",
    "dependency-check": "^3.1.0",
    "standard": "^11.0.1"
  }
}
